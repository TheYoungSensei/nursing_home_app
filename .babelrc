{
  "presets": [
    ["env",
      {
        "modules": false
      }
    ],
    "react",
    "stage-2",
    "flow",
    "es2015"
  ],
  "plugins": [
    "transform-regenerator",
    "transform-async-to-generator",
    "transform-class-properties"
  ],
  "env": {
    "production": {
      "presets": [
        "react-optimize"
      ],
      "comments": false
    }
  }
}
